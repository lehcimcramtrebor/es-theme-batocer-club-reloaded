<gridtile name="default">
<backgroundColor>FFFFFF00</backgroundColor>
<backgroundCornerSize>9 9</backgroundCornerSize>
<selectionMode>video</selectionMode>
<padding>5 5</padding>
<imageColor>ffffff</imageColor>
<reflexion>0 0</reflexion>	  
</gridtile>
	
<gridtile name="selected">
<backgroundCenterColor>46dcdc66</backgroundCenterColor>
<backgroundEdgeColor>feed00</backgroundEdgeColor>	
<imageColor>FFFFFFFF</imageColor>
</gridtile>

<ninepatch name="gridtile.background:selected">
<animateColor>00000000</animateColor>
<animateColorTime>350</animateColorTime>
</ninepatch>

<control name="md_marquee,0manual,0savestate,0cheevos,0kidgame,0hidden,line,line2,md_name,md_lbl_players,md_players,md_video,md_lbl_releasedate,md_releasedate,md_lbl_genre,md_genre,md_lbl_playcount,md_playcount,md_rating,md_genre,md_description" ifSubset="control:yes">
<storyboard event="open">
<animation property="opacity" from="0" to="0" duration="250" mode="easeIn"/>
<animation property="opacity" from="0" to="1" duration="400" mode="easeIn" begin="250"/>
<animation property="offsetX" from="-0.5" to="0" duration="400" mode="easeOutCubic" begin="250"/>
</storyboard>
<storyboard event="activateNext">
<animation property="offsetY" from="0.1" duration="250" mode="easeOutCubic"/>
<animation property="opacity" from="0" duration="250" mode="easeOutCubic"/>
</storyboard>
<storyboard event="activatePrev">
<animation property="opacity" from="0" duration="250" mode="easeOutCubic"/>
<animation property="offsetY" from="-0.1" duration="250" mode="easeOutCubic"/>
</storyboard>
<storyboard event="deactivateNext">
<animation property="opacity" to="0" duration="50" mode="easeIn"/>
</storyboard>
<storyboard event="deactivatePrev">
<animation property="opacity" to="0" duration="50" mode="easeIn"/>
</storyboard>
</control>


<control name="md_manual,md_savestate,md_cheevos,md_kidgame,md_hidden" ifSubset="control:yes">
<storyboard event="open">
<animation property="opacity" from="0" to="0" duration="250" mode="easeIn"/>
<animation property="opacity" from="0" to="1" duration="400" mode="easeIn" begin="250"/>
<animation property="offsetX" from="-0.5" to="0" duration="400" mode="easeOutCubic" begin="250"/>
</storyboard>
<storyboard event="activateNext">
<animation property="opacity" from="0" to="0" duration="350" mode="linear"/>
<animation property="opacity" from="0" to="1" begin="350" duration="250" mode="linear"/>
</storyboard>
<storyboard event="activatePrev">
<animation property="opacity" from="0" to="0" duration="350" mode="linear"/>
<animation property="opacity" from="0" to="1" begin="350" duration="250" mode="linear"/>
</storyboard>
<storyboard event="deactivateNext">
<animation property="opacity" to="0" duration="50" mode="easeIn"/>
</storyboard>
<storyboard event="deactivatePrev">
<animation property="opacity" to="0" duration="50" mode="easeIn"/>
</storyboard>
</control>

</view>

<view name="system,basic,detailed,video,grid,gamecarousel">

<image name="scanlines" extra="true">
<origin>0.5 0.5</origin>
<size>0.6171875 0.712962962962963</size>
<pos>0.64140625 0.5564814814814815</pos>
<size ifSubset="screenRatio:43">0.61953125 0.50390625</size>
<pos ifSubset="screenRatio:43">0.641015625 0.5400390625</pos>
<zIndex>6</zIndex>
<tile>true</tile>
<visible ifSubset="staticEffectApply:screenApply">false</visible>
</image>

<image name="wallpaper" extra="true">
<linearSmooth>true</linearSmooth>
<origin>0 0</origin>
<pos>0 0</pos>
<maxSize>99 1</maxSize>
<zIndex>0</zIndex>                 
<color>ffffff</color>
<colorEnd>ffffff</colorEnd>
<gradientType>vertical</gradientType>
<storyboard>
<animation property="color" to="00ffff" duration="2500" begin="350" autoreverse="true" repeat="forever" mode="easeIn"/>
<animation property="colorEnd" to="ffff50" duration="5000" begin="350" autoreverse="true" repeat="forever" mode="easeIn"/>
</storyboard>
</image>                    

<image name="backOverlay" extra="true">
<linearSmooth>true</linearSmooth>
<origin>1 0.5</origin>
<pos>1 0.5</pos>
<maxSize>1 99</maxSize>
<zIndex>1</zIndex>                 
<path>./_inc/images/backOverlay.png</path>
<color>121212</color>
<colorEnd>121212</colorEnd>
<gradientType>vertical</gradientType>
</image>                    

<image name="overlay" extra="true">
<linearSmooth>true</linearSmooth>
<origin>1 0.5</origin>
<pos>1 0.5</pos>
<maxSize>1 99</maxSize>
<zIndex>7</zIndex>                 
<path>./_inc/images/overlay.png</path>
<path ifSubset="glass:no">./_inc/images/overlay2.png</path>
</image>                    

<image name="white" extra="true">
<linearSmooth>true</linearSmooth>
<origin>1 0.5</origin>
<pos>1 0.5</pos>
<maxSize>1 99</maxSize>
<zIndex>8</zIndex>                 
<path>./_inc/images/white.png</path>
<color>ffffff</color>
<colorEnd>ffffff</colorEnd>
<gradientType>vertical</gradientType>
<storyboard>
<animation property="opacity" to="0.8" duration="15" autoreverse="true" repeat="forever" mode="bump"/>
</storyboard>
</image>                    

<image name="invader,snakervill,batocera,lightbatocera,club,lightclub" extra="true" ifSubset="screenRatio:169">
<linearSmooth>true</linearSmooth>
<size>1 1</size>
<pos>0 0</pos>
</image>

<image name="batocera,lightbatocera,club,lightclub" extra="true" ifSubset="screenRatio:43">
<linearSmooth>true</linearSmooth>
<size>1.3 1</size>
<pos>1.1 0</pos>
<origin>1 0</origin>
</image>

<image name="snakervill" extra="true" ifSubset="screenRatio:43">
<linearSmooth>true</linearSmooth>
<size>1.3 1</size>
<pos>1.08 0.1</pos>
<origin>1 0</origin>
</image>

<image name="invader" extra="true" ifSubset="screenRatio:43">
<linearSmooth>true</linearSmooth>
<size>1.3 1</size>
<pos>1 -0.1</pos>
<origin>1 0</origin>
</image>

<image name="invader" extra="true">
<path>./_inc/images/invader.png</path>
<zIndex>100</zIndex>
<color>ffffff</color>
<storyboard>
<animation property="opacity" from="1" to="1" duration="350" mode="Linear"/>
<animation property="opacity" to="0" duration="1000" begin="350" autoreverse="true" repeat="forever" mode="easeIn"/>
</storyboard>
</image>

<image name="snakervill" extra="true">
<path>./_inc/images/snakervill.png</path>
<zIndex>150</zIndex>
<storyboard>
<animation property="offsetX" from="1" to="1" duration="250" mode="easeOut"/>
<animation property="offsetX" from="1" to="0" duration="250" begin="500" mode="easeOut"/>
<animation property="offsetY" to="0.01" duration="250" autoreverse="true" repeat="forever" mode="easeIn"/>
</storyboard>

</image>
<image name="batocera" extra="true">
<path>./_inc/images/batocera.png</path>
<zIndex>140</zIndex>
</image>

<image name="lightbatocera" extra="true">
<path>./_inc/images/lightbatocera.png</path>
<zIndex>120</zIndex>
<color>ffffff</color>
<colorEnd>ffffff</colorEnd>
<storyboard>
<animation property="opacity" to="0.9" duration="60" autoreverse="true" repeat="forever" mode="easeIn"/>
<animation property="color" to="ffffffe6" duration="30" autoreverse="true" repeat="forever" mode="easeIn"/>
<animation property="colorEnd" to="ff0000e6" duration="15" autoreverse="true" repeat="forever" mode="easeIn"/>
</storyboard>

</image>
<image name="club" extra="true">
<path>./_inc/images/club.png</path>
<zIndex>130</zIndex>
</image>

<image name="lightclub" extra="true">
<path>./_inc/images/lightclub.png</path>
<zIndex>110</zIndex>
<color>ffffff</color>
<colorEnd>ffffff</colorEnd>
<storyboard>
<animation property="opacity" to="0.9" duration="60" autoreverse="true" repeat="forever" mode="easeIn"/>
<animation property="color" to="ffffffe6" duration="30" autoreverse="true" repeat="forever" mode="easeIn"/>
<animation property="colorEnd" to="ff0000e6" duration="15" autoreverse="true" repeat="forever" mode="easeIn"/>
</storyboard>
</image>

</view>

<include>./_inc/specifics/${system.theme}.xml</include>

</theme>