<?xml version="1.0" encoding="UTF-8"?>
<theme defaultView="video">
<formatVersion>7</formatVersion>

<include>./LABEL_CUSTOM.xml</include>
<include>./_inc/localisation/lang.xml</include>
<include>./_inc/localisation/lang-${lang}.xml</include>

<subset name="screenRatio" displayName="${lang.screenratio}">
<include name="169" displayName="${lang.169}"/>
<include name="43" displayName="${lang.43}"/>
</subset>

<subset name="transition" displayName="${lang.transition}">
<include name="no" displayName="${lang.no}"/>
<include name="yes" displayName="${lang.yes}"/>
</subset>

<subset name="staticEffect" displayName="${lang.staticEffect}">
<include name="no" displayName="${lang.no}"/>
<include name="scanlines" displayName="${lang.scanlines}"/>
<include name="scanlinesFine" displayName="${lang.scanlinesFine}"/>
<include name="scanlinesVertical" displayName="${lang.scanlinesVertical}"/>
<include name="scanlinesVerticalFine" displayName="${lang.scanlinesVerticalFine}"/>
<include name="crt" displayName="${lang.crt}"/>
<include name="phosphore" displayName="${lang.phosphore}"/>
<include name="phosphorePlus" displayName="${lang.phosphorePlus}"/>
</subset>

<subset name="glass" displayName="${lang.glass}">
<include name="yes" displayName="${lang.yes}"/>
<include name="no" displayName="${lang.no}"/>
</subset>

<subset name="staticEffectApply" displayName="${lang.staticEffectApply}" ifSubset="staticEffect:scanlines|scanlinesFine|scanlinesVertical|scanlinesVerticalFine|crt|phosphore|phosphorePlus">
<include name="screenApply" displayName="${lang.screenApply}"/>
<include name="artworksApply" displayName="${lang.artworksApply}"/>
</subset>

<subset name="themeBackground" displayName="${lang.themeBackground}">
<include name="mixed" displayName="${lang.mixed}"/>
<include name="originalWallpaper" displayName="${lang.originalWallpaper}"/>
<include name="underground" displayName="${lang.underground}"/>
</subset>	

<subset name="control" displayName="${lang.control}">
<include name="yes" displayName="${lang.yes}"/>
<include name="no" displayName="${lang.no}"/>
</subset>	

<subset name="help" displayName="${lang.help}">
<include name="yes" displayName="${lang.yes}"/>
<include name="no" displayName="${lang.no}"/>
</subset>

<subset name="helpIcons" displayName="${lang.helpIcons}" ifSubset="help:yes">
<include name="defaultIcons" displayName="${lang.defaultIcons}"/>
<include name="arcadeIcons" displayName="${lang.arcadeIcons}"/>
<include name="psxIcons" displayName="${lang.psxIcons}"/>
<include name="snesIcons" displayName="${lang.snesIcons}"/>
<include name="snesAltIcons" displayName="${lang.snesAltIcons}"/>
<include name="xboxIcons" displayName="${lang.xboxIcons}"/>
<include name="xboxOneIcons" displayName="${lang.xboxOneIcons}"/>
</subset>

<subset name="systemText" displayName="${lang.systemText}">
<include name="systemTextBind" displayName="${lang.systemTextBind}"/>
<include name="systemTextStandard" displayName="${lang.systemTextStandard}"/>
<include name="systemTextFull" displayName="${lang.systemTextFull}"/>
<include name="systemTextFullCheevos" displayName="${lang.systemTextFullCheevos}"/>
<include name="systemTextLabel" displayName="${lang.systemTextLabel}"/>
<include name="systemTextCheevos" displayName="${lang.systemTextCheevos}"/>
<include name="justManufacturer" displayName="${lang.justManufacturer}"/>
<include name="justYear" displayName="${lang.justYear}"/>
<include name="notDisplay" displayName="${lang.notDisplay}"/>
</subset>

<subset name="RandomVideos" displayName="${lang.randomVideos}" appliesTo="basic,detaile,video,grid,gamecarousel">
<include name="BatoceraClub" displayName="${lang.BatoceraClubVideos}"/>
<include name="SnapsVideos" displayName="${lang.snapsVideos}"/>
</subset>
		
